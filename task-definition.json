{
  "family": "iucds-middleware",
  "executionRoleArn": "arn:aws:iam::410123189863:role/iucds-ecs-task-role",
  "containerDefinitions": [
      {
          "name": "iucds-middleware-container",
          "image": "docker.pkg.github.com/uecit/email-adapter/iucds-middleware:latest",
          "cpu": 512,
          "portMappings": [
            {
              "hostPort": 8888,
              "protocol": "tcp",
              "containerPort": 8888
            }
          ],
          "memory": 1024,
          "essential": true,
          "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
              "awslogs-group": "/ecs/iucds-middleware",
              "awslogs-region": "eu-west-2",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}